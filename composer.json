{
    "name": "lifetrenz/transcendz",
    "version": "0.5.0",
    "description": "Lifetrenz library project for PHP utility.",
    "type": "library",
    "license": "AFL-3.0",
    "autoload": {
        "psr-4": {
            "Lifetrenz\\Transcendz\\": "src/"
        }
    },
